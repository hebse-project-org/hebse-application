{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:decrypt": "uvicorn app.bootstrap:bootstrap --port 9001",
    "dev:backend": "cd ../backend && python -m uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "concurrently \"node --experimental-strip-types src/tunnel/tunnel-server.ts\" \"vite\"",
    "prod:frontend": "concurrently \"node --experimental-strip-types src/tunnel/tunnel-server.ts\" \"vite\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "@mui/x-data-grid": "^7.27.3",
    "cors": "^2.8.5",
    "fastapi": "^0.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "ssh2": "^1.16.0",
    "tunnel-ssh": "^5.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/ssh2": "^1.15.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "eslint-plugin-unicorn": "^57.0.0",
    "globals": "^15.11.0",
    "jsdom": "^26.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^6.2.1",
    "vitest": "^3.1.1"
  }
}
